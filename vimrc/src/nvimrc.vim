set runtimepath+=~/.vim,~/.vim/after
set packpath+=~/.vim

" before
" ---

" python
let g:python2_host_prog = '/usr/bin/python2.7'
let g:python3_host_prog = '/usr/bin/python3.5'

" ruby
let g:ruby_host_prog = '~/.rvm/gems/ruby-2.4.0/bin/neovim-ruby-host'

" source
" ---
source ~/.vim/vimrc

" after
" ---
set guicursor=
